#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/yinhao/Desktop/Node/04_邂逅webpack/08_webpack搭建本地服务器/node_modules/.pnpm/webpack-cli@5.1.4_webpack@5.91.0/node_modules/webpack-cli/bin/node_modules:/Users/yinhao/Desktop/Node/04_邂逅webpack/08_webpack搭建本地服务器/node_modules/.pnpm/webpack-cli@5.1.4_webpack@5.91.0/node_modules/webpack-cli/node_modules:/Users/yinhao/Desktop/Node/04_邂逅webpack/08_webpack搭建本地服务器/node_modules/.pnpm/webpack-cli@5.1.4_webpack@5.91.0/node_modules:/Users/yinhao/Desktop/Node/04_邂逅webpack/08_webpack搭建本地服务器/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/yinhao/Desktop/Node/04_邂逅webpack/08_webpack搭建本地服务器/node_modules/.pnpm/webpack-cli@5.1.4_webpack@5.91.0/node_modules/webpack-cli/bin/node_modules:/Users/yinhao/Desktop/Node/04_邂逅webpack/08_webpack搭建本地服务器/node_modules/.pnpm/webpack-cli@5.1.4_webpack@5.91.0/node_modules/webpack-cli/node_modules:/Users/yinhao/Desktop/Node/04_邂逅webpack/08_webpack搭建本地服务器/node_modules/.pnpm/webpack-cli@5.1.4_webpack@5.91.0/node_modules:/Users/yinhao/Desktop/Node/04_邂逅webpack/08_webpack搭建本地服务器/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../bin/cli.js" "$@"
else
  exec node  "$basedir/../../bin/cli.js" "$@"
fi
