#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/yinhao/Desktop/Node/04_邂逅webpack/08_webpack搭建本地服务器/node_modules/.pnpm/rimraf@5.0.7/node_modules/rimraf/dist/esm/node_modules:/Users/yinhao/Desktop/Node/04_邂逅webpack/08_webpack搭建本地服务器/node_modules/.pnpm/rimraf@5.0.7/node_modules/rimraf/dist/node_modules:/Users/yinhao/Desktop/Node/04_邂逅webpack/08_webpack搭建本地服务器/node_modules/.pnpm/rimraf@5.0.7/node_modules/rimraf/node_modules:/Users/yinhao/Desktop/Node/04_邂逅webpack/08_webpack搭建本地服务器/node_modules/.pnpm/rimraf@5.0.7/node_modules:/Users/yinhao/Desktop/Node/04_邂逅webpack/08_webpack搭建本地服务器/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/yinhao/Desktop/Node/04_邂逅webpack/08_webpack搭建本地服务器/node_modules/.pnpm/rimraf@5.0.7/node_modules/rimraf/dist/esm/node_modules:/Users/yinhao/Desktop/Node/04_邂逅webpack/08_webpack搭建本地服务器/node_modules/.pnpm/rimraf@5.0.7/node_modules/rimraf/dist/node_modules:/Users/yinhao/Desktop/Node/04_邂逅webpack/08_webpack搭建本地服务器/node_modules/.pnpm/rimraf@5.0.7/node_modules/rimraf/node_modules:/Users/yinhao/Desktop/Node/04_邂逅webpack/08_webpack搭建本地服务器/node_modules/.pnpm/rimraf@5.0.7/node_modules:/Users/yinhao/Desktop/Node/04_邂逅webpack/08_webpack搭建本地服务器/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../dist/esm/bin.mjs" "$@"
else
  exec node  "$basedir/../../dist/esm/bin.mjs" "$@"
fi
