#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/yinhao/Desktop/Node/04_邂逅webpack/08_webpack搭建本地服务器/node_modules/.pnpm/ansi-html-community@0.0.8/node_modules/ansi-html-community/bin/node_modules:/Users/yinhao/Desktop/Node/04_邂逅webpack/08_webpack搭建本地服务器/node_modules/.pnpm/ansi-html-community@0.0.8/node_modules/ansi-html-community/node_modules:/Users/yinhao/Desktop/Node/04_邂逅webpack/08_webpack搭建本地服务器/node_modules/.pnpm/ansi-html-community@0.0.8/node_modules:/Users/yinhao/Desktop/Node/04_邂逅webpack/08_webpack搭建本地服务器/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/yinhao/Desktop/Node/04_邂逅webpack/08_webpack搭建本地服务器/node_modules/.pnpm/ansi-html-community@0.0.8/node_modules/ansi-html-community/bin/node_modules:/Users/yinhao/Desktop/Node/04_邂逅webpack/08_webpack搭建本地服务器/node_modules/.pnpm/ansi-html-community@0.0.8/node_modules/ansi-html-community/node_modules:/Users/yinhao/Desktop/Node/04_邂逅webpack/08_webpack搭建本地服务器/node_modules/.pnpm/ansi-html-community@0.0.8/node_modules:/Users/yinhao/Desktop/Node/04_邂逅webpack/08_webpack搭建本地服务器/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../bin/ansi-html" "$@"
else
  exec node  "$basedir/../../bin/ansi-html" "$@"
fi
